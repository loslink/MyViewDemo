    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <Button
                android:id="@+id/bt_recycler"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="拼图"
                android:layout_margin="10dp"/>
            <Button
                android:id="@+id/bt_1"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="曲线特效"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_2"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="旋转动画"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_3"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="清理圆圈动画"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_4"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="文字"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_5"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="风扇动画"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_6"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="清理"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_7"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="CPU动画"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_8"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="开关"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_9"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="浮球"
                android:layout_margin="10dp"/>

            <Button
                android:id="@+id/bt_10"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="清理新动画"
                android:layout_margin="10dp"/>

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>


</android.support.constraint.ConstraintLayout>
